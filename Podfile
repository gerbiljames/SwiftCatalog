# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'SwiftCatalog' do
  # Uncomment the next line if you're using Swift or would like to use dynamic frameworks
  use_frameworks!

  # Pods for SwiftCatalog
  pod 'FacebookCore', :git => 'https://github.com/gerbiljames/facebook-sdk-swift.git', :branch => 'swift-3.0'
  pod 'FacebookLogin', :git => 'https://github.com/gerbiljames/facebook-sdk-swift.git', :branch => 'swift-3.0'
  pod 'FacebookShare', :git => 'https://github.com/gerbiljames/facebook-sdk-swift.git', :branch => 'swift-3.0'
end
